{
    "version": 2,
    "buildCommand": "npm run build:prod",
    "outputDirectory": "dist",
    "framework": "astro",
    "redirects": [
        {
            "source": "/admin",
            "destination": "/404",
            "permanent": false
        },
        {
            "source": "/admin/:path*",
            "destination": "/404",
            "permanent": false
        }
    ]
}
