# Environment Variables
# Copy this file to .env and fill in your values

# Admin page access key
# Access admin at: /admin?key=YOUR_KEY_HERE
ADMIN_KEY=your-secret-admin-key-here

# Admin API Configuration (for local development)
# Admin API server URL
ADMIN_API_URL=http://localhost:4322/api
# Admin API server port (should match admin-server.mjs)
ADMIN_API_PORT=4322
